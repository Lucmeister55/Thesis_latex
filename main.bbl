      \field{year}{2020}
      \field{pages}{3233}
      \range{pages}{1}
      \keyw{DNA repair; P-glycoprotein; cancer; chemotherapeutics; drug metabolism; epigenetic alterations; growth factors; inhibitors; microRNA; multidrug resistance}
